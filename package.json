{
  "name": "fis3-angular2-app",
  "version": "0.0.0",
  "description": "Angular2 App Based on fis3",
  "scripts": {
    "server": "npm run server.start",
    "server.start": "fis3 server start",
    "release": "npm run release.dev",
    "release.dev": "fis3 release -w",
    "release.lc": "fis3 release lc -d ./dist/lc",
    "release.qa": "fis3 release qa -d ./dist/qa",
    "release.pr": "fis3 release pr -d ./dist/pr",
    "clean": "fis3 server stop && fis3 server clean && node npm_scripts/clean.js",
    "clean.npm": "node npm_scripts/clean_npm.js",
    "clean.all": "npm run clean && npm run clean.npm",
    "start": "npm run clean && npm run server",
    "dev": "npm run release",
    "build": "npm run build.lc",
    "build.lc": "npm run release.lc",
    "build.qa": "npm run release.qa",
    "build.pr": "npm run release.pr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luozt/fis3-angular2-app.git"
  },
  "bugs": {
    "url": "https://github.com/luozt/fis3-angular2-app/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "keywords": [
    "fis3",
    "ng2",
    "angular2"
  ],
  "author": "LUOZHITAO",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^2.1.0",
    "fis-mod": "^1.0.1",
    "fis-optimizer-htmlmin": "^0.1.2",
    "fis-optimizer-png-compressor": "^0.1.6",
    "fis-parser-jade": "0.0.12",
    "fis-parser-less": "^0.1.3",
    "fis-parser-pug": "0.0.1",
    "fis-parser-react": "^0.13.3",
    "fis-postpackager-replace": "0.0.3",
    "fis3": "^3.4.13",
    "fis3-deploy-local-supply": "0.0.2",
    "fis3-hook-commonjs": "^0.1.22",
    "fis3-hook-node_modules": "^2.2.3",
    "fis3-hook-relative": "^1.0.7",
    "fis3-parser-typescript": "^0.2.5",
    "fis3-postpackager-loader": "^1.5.2",
    "fis3-preprocessor-cssprefixer": "0.0.2",
    "fis3-preprocessor-js-require-css": "^0.1.0",
    "fis3-preprocessor-js-require-file": "^0.1.0",
    "fis3-preprocessor-ng2-inline": "0.0.1",
    "is-buffer": "^1.1.3",
    "liftoff": "^2.2.1",
    "process": "^0.11.5",
    "typings": "^1.3.1"
  },
  "dependencies": {
    "@angular/common": "~2.4.1",
    "@angular/compiler": "~2.4.1",
    "@angular/core": "~2.4.1",
    "@angular/forms": "~2.4.1",
    "@angular/http": "~2.4.1",
    "@angular/platform-browser": "~2.4.1",
    "@angular/platform-browser-dynamic": "~2.4.1",
    "@angular/platform-server": "~2.4.1",
    "@angular/router": "~3.4.1",
    "@angularclass/conventions-loader": "^1.0.2",
    "@angularclass/hmr": "~1.2.2",
    "@angularclass/hmr-loader": "~3.0.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "es6-shim": "^0.35.2",
    "http-server": "^0.9.0",
    "ie-shim": "^0.1.0",
    "jasmine-core": "^2.5.2",
    "jquery": "^3.1.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "~5.0.2",
    "zone.js": "~0.7.4"
  }
}
